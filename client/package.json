{
  "name": "Recursive Bet",
  "version": "0.2.1",
  "description": "This is a demonstrative Ethereum DApp that allows users to bet and outbet others.",
  "private": true,
  "homepage": "http://Tom2718.github.io/recursive-bet",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "bignumber.js": "^7.2.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4",
    "truffle-contract": "^4.0.0-next.0",
    "typeface-roboto": "0.0.54",
    "web3": "^1.0.0-beta.35"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "link-contracts": "run-script-os",
    "link-contracts:linux:darwin": "cd src && ln -s ../../build/contracts contracts",
    "link-contracts:win32": "cd src && mklink /D contracts ..\\..\\build\\contracts",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "run-script-os": "^1.0.3"
  }
}
